    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.54.0",
    "eslint-config-expo": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "detox": "^20.13.0",
    "detox-cli": "^20.13.0"
