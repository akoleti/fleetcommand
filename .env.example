# Neon Database
DATABASE_URL=postgresql://user:password@...neon.tech/fleetcommand?sslmode=require

# Auth
JWT_SECRET=your_jwt_secret_here
JWT_REFRESH_SECRET=your_refresh_secret_here

# Redis (Upstash)
UPSTASH_REDIS_REST_URL=https://...upstash.io
UPSTASH_REDIS_REST_TOKEN=...

# AWS S3
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1
S3_BUCKET_DELIVERY=fleet-delivery-media
S3_BUCKET_INSURANCE=fleet-insurance-docs
S3_BUCKET_REPORTS=fleet-reports

# Firebase FCM
FIREBASE_SERVICE_ACCOUNT=...

# Twilio SMS
TWILIO_ACCOUNT_SID=...
TWILIO_AUTH_TOKEN=...
TWILIO_PHONE_NUMBER=+1234567890

# SendGrid Email
SENDGRID_API_KEY=...
SENDGRID_FROM_EMAIL=noreply@fleetcommand.com

# GPS Tracker
GPS_WEBHOOK_SECRET=your_gps_webhook_secret

# Maps
NEXT_PUBLIC_MAPS_API_KEY=...

# Vercel Cron
VERCEL_CRON_SECRET=your_cron_secret

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development
